<div class="container mx-auto" style="max-width: 640px">
  <h1 class="mb-4">Mengedit Mata Kuliah</h1>

  <%= render 'form', mata_kuliah: @mata_kuliah %>

  <%= link_to mata_kuliahs_path, class: "btn btn-primary" do %>
    <i class="fas fa-arrow-left"></i> Kembali
  <% end %>
  <%= link_to @mata_kuliah, class: "btn btn-primary" do %>
    <i class="fas fa-eye"></i> Lihat
  <% end %>
</div>
