<div class="container mx-auto" style="max-width: 640px">
  <h1 class="mb-4">Mengedit Jadwal Mata Kuliah</h1>

  <%= render 'form', jadwal_mata_kuliah: @jadwal_mata_kuliah %>

  <%= link_to jadwal_mata_kuliahs_path, class: "btn btn-primary" do %>
    <%= octicon "arrow-left", class: "inline-block" %> Kembali
  <% end %>
  <%= link_to @jadwal_mata_kuliah, class: "btn btn-primary" do %>
    <%= octicon "eye", class: "inline-block" %> Lihat
  <% end %>
</div>
